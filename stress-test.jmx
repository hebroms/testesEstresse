          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion(Taxa de Erro)">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Obtém as amostras
def sampleResults = ctx.getPreviousResult().getSubResults()
def totalRequests = sampleResults.size()
def failedRequests = sampleResults.count { it.isSuccessful() == false }
def errorRate = (failedRequests / totalRequests) * 100

if (errorRate &gt; 5) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(&quot;Taxa de erro superior a 5%: ${errorRate}%&quot;)
}
</stringProp>
          </JSR223Assertion>
