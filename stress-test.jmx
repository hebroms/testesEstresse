          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion(Critérios de Aceitação)">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Obtém os resultados das amostras
def sampleResults = ctx.getPreviousResult().getSubResults()
def averageResponseTime = sampleResults.collect { it.getTime() }.sum() / sampleResults.size()

if (averageResponseTime &gt; 3000) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(&quot;Tempo médio de resposta acima de 3 segundos: ${averageResponseTime}ms&quot;)
}
</stringProp>
          </JSR223Assertion>
